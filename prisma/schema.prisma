generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
}

model user {
  id        Int       @id @default(autoincrement())
  name      String
  email     String    @unique(map: "User_email_key")
  createdAt DateTime  @default(now())
  role      user_role @default(USER)
}

enum user_role {
  USER
  ADMIN
}
